{
    "background": {
        "page": "background.html"
    },
    "options_page": "options.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "description": "DynamicGazo",
    "icons": {
        "128": "image/icon.png",
        "48" : "image/icon.png",
        "16" : "image/icon.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "image/icon-l.png",
            "38": "image/icon-l.png"
        },
        "default_title": "DynamicGazo",
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "name": "DynamicGazo",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": [
            "lib/jquery-ui.min.css",
            "css/insite.css"
        ],
        "js": [
            "lib/jquery.js",
            "lib/jquery-ui.min.js",
            "build/bundle.js",
            "build/common.js",
            "build/inline_viewer.js",
            "build/content_script.js"
        ],
        "run_at": "document_end"
    }],

    "web_accessible_resources": [
        "image/x.png",
        "image/linkcard/*"
    ],

    "permissions": [
        "contextMenus",
        "pageCapture",
        "tabs",
        "activeTab",
        "cookies",
        "<all_urls>"
    ],
    "version": "0.2.5"
}
